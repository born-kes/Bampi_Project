<IfModule mod_deflate.c>
	SetOutputFilter DEFLATE
</IfModule>

 php_flag magic_quotes_gpc off

 RewriteEngine on
#rder allow,deny
#deny from all

#ErrorDocument 404 /lekkicms/index.php

 RewriteRule ^(.*)\.$  /aa_projekt/index.php?page=$1 [QSA,L]
 RewriteRule ^(.*)\.html?$  /aa_projekt/index.php?page=$1 [QSA,L]
#RewriteRule ^(.*)\.php?$  /nowaStudnia/index.php?q=$1 [QSA,L]
#RewriteRule ^(.*)\.js?$   /nowaStudnia/index.php?q=js&js=$1 [QSA,L]
 RewriteRule ^(.*)\.json?$ /aa_projekt/index.php?typ=json&page=$1 [QSA,L]
#RewriteRule ^([^.]*)\?$    /nowaStudnia/index.php?q=$1 [QSA,L]
 RewriteRule ^([^.]*)\admin\$    /aa_projekt/admin/index.php?page=$1 [QSA,L]

